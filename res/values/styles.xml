<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    
    <style name="block_style">
    	<item name="android:layout_width">60dp</item>
        <item name="android:layout_height">wrap_content</item>   
    </style>
    
    <style name="bymatko_null">
        <item name="android:layout_width">18dp</item>
        <item name="android:layout_height">18dp</item>
        <item name="android:layout_margin">6dp</item>
        <item name="android:src">@null</item>
        <item name="android:contentDescription">@string/empty</item>
    </style> 
    
    <style name="bymatko_on" parent="@style/bymatko_null">
        <item name="android:src">@drawable/bymatko_black_on</item>
        <item name="android:contentDescription">@string/on</item>
    </style>
    
    <style name="bymatko_off" parent="@style/bymatko_null">
        <item name="android:src">@drawable/bymatko_black_off</item>
        <item name="android:contentDescription">@string/off</item>
    </style>
    
</resources>